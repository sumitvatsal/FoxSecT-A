      <!--<threshold value="ALL"/>-->
      <to value="Enter Your Mail" />
      <from value="test@foxsec.ee" />
      <subject value="FoxSec Mail" />
      <smtpHost value="mail.helmes.ee" />
      <bufferSize value="512" />
      <lossy value="false" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%newline%date - [%-5level]: %message%newline%newline%newline" />
      </layout>
